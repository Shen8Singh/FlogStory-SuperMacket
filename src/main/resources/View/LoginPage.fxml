            <Button layoutX="102.0" layoutY="381.0" mnemonicParsing="false" onAction="#loginBtn" prefHeight="30.0" prefWidth="103.0" style="-fx-background-color: #C45457; -fx-background-radius: 5px;" text="SingIn" textFill="WHITE">
               <font>
                  <Font name="FreeSerif" size="13.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="CLOSED_HAND" />
               </cursor>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
