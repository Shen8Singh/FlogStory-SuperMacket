<AnchorPane fx:id="logingAnchorPane" prefHeight="446.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ZenVeus.JavaFx.Controllers.LoginPageController">
   <children>
      <AnchorPane prefHeight="450.0" prefWidth="350.0">
         <children>
            <ImageView fitHeight="450.0" fitWidth="331.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/logImage.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="320.0" prefHeight="450.0" prefWidth="318.0">
         <children>
            <Text fill="#cd6363" layoutX="109.0" layoutY="164.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">
               <font>
                  <Font name="FreeMono Bold" size="30.0" />
               </font>
            </Text>
            <TextField fx:id="uNameText" layoutX="74.0" layoutY="246.0" promptText="User Name" style="-fx-background-color: transpanent; -fx-border-color: #C4393D; -fx-border-width: 0px 0px 2px 0px;" />
            <PasswordField fx:id="uPassText" layoutX="74.0" layoutY="296.0" promptText="User Password" style="-fx-background-color: transpanent; -fx-border-color: #C4393D; -fx-border-width: 0px 0px 2px 0px;" />
